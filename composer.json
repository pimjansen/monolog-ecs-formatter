{
    "name": "pimjansen/monolog-ecs-formatter",
    "description": "Monolog formatter for Elastic Common Schema",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "adbario/php-dot-notation": "^2.2",
        "monolog/monolog": "^2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4",
        "symfony/yaml": "^5.2",
        "twig/twig": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "ECS\\Formatter\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }

}
